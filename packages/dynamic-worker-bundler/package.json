{
  "name": "dynamic-worker-bundler",
  "version": "0.0.1",
  "description": "Bundle and resolve modules for Cloudflare Worker Loader binding",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "es-module-lexer": "^2.0.0",
    "esbuild-wasm": "^0.24.2",
    "resolve.exports": "^2.0.3",
    "sucrase": "^3.35.1"
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist"
  ]
}
